      "micro_map_postprocessing": {
        "1": {
          "__comment": "Boolean, false checks only orthogonal neighbours while true also diagonal (true is needed for full traversability).",
          "method": "force_ziggurat",
          "include_diagonal": true
        },
        "2": {
          "__comment": "Self-explanatory. This is pretty much the radius of the resulting 'plateau' in chunks.",
          "method": "overflow_lower",
          "include_diagonal": false,
          "height_threshold": 60,
          "iterations": 1
        },
        "3": {
          "__comment": "Decides what to change the height to, 'min' chooses the lowest of neighbours, 'max' the highest, 'random' picks one of them (and is theoretically slower but most machines should not show a difference).",
          "method": "remove_loners",
          "mode": "max"
        }
      },
